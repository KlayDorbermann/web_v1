{
    "presets": [
      [
        "next/babel",
        {
          "preset-react": {
            "runtime": "automatic",
            "importSource": "@emotion/react"
          }
        }
      ]
    ],
    "plugins": [
      "@emotion/babel-plugin",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@src/*": "*",
            "@common/*": "common/*",
            "@assets/*": "assets/*",
            "@pages/*": "pages/*",
            "@domains/*": "domains/*",
            "@User/*": "domains/User/*",
            "@Errors/*": "domains/Errors/*"
          }
        }
      ]
    ]
  }